  "devDependencies": {
    "acorn": "^6.2.0",
    "babel-jest": "^24.8.0",
    "babel-preset-jaid": "^6.8.1",
    "browserslist-config-jaid-node": "^2.2.1",
    "eslint": "^6.0.1",
    "eslint-config-jaid": "^1.18.0",
    "fs-readdir-recursive": "^1.1.0",
    "jest": "^24.8.0",
