    "publishimo": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/register": "^7.0.0",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.0",
    "eslint-config-jaid": "^1.8.1",
    "fs-readdir-recursive": "^1.1.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-config-node": "^1.0.0",
    "babel-preset-jaid": "^1.0.1"
  }
